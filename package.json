{
  "name": "counter-test-datagile",
  "description": "Тестовое задание со счетчиками для Datagile",
  "keywords": [
    "React",
    "Typescript",
    "Redux",
    "Tailwind"
  ],
  "files": [
    "dist",
    "dist/*.css"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/counter-test-datagile.umd.js",
  "module": "./dist/counter-test-datagile.es.js",
  "style": "style.css",
  "exports": {
    ".": {
      "import": "./dist/counter-test-datagile.es.js",
      "require": "./dist/counter-test-datagile.umd.js"
    }
  },
  "author": {
    "name": "Ilya Sudakov",
    "email": "ilyasudakov.dev@gmail.com",
    "url": "https://ilyasudakov.vercel.app"
  },
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-vercel": "tsc && vite build --config vite.vercel-config.ts",
    "start": "npm i && vite --open",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "react-redux": "^8.0.2",
    "vite-plugin-svgr": "^2.2.1"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/rollup-plugin-postcss": "^3.1.4",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "postcss": "^8.4.16",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vite-plugin-dts": "^1.4.1"
  }
}
